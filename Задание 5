a = float(input("Введите значение выручки - "))
b = float(input("Введите значение издержек - "))
c = a - b
if c > 0:
    print(f"Прибыль {c} ")
    print(f"Рентабельность {100 * (c / b):.1f}%")
    d = int(input("Количество работников в компании "))
    print(f"Каждый получит по {c / d} ")
elif c < 0:
    print(f"Убыток {-c}")
else:
    print("0")

# a - выручка
# b - издержки
# c - выручка - издержки
# d - кол-во персонала
